{
  "name": "pact-mock-js",
  "version": "1.1.1",
  "description": "A Library to build Pact interactions by leveraging existing mocks",
  "keywords": [
    "typescript",
    "template",
    "jest"
  ],
  "author": {
    "name": "Ludovic Dorival"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/ludorival/pact-mock-js.git"
  },
  "exports": {
    "./core": {
      "default": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./msw": {
      "default": "./dist/msw/index.js",
      "types": "./dist/msw/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && tsc --build --verbose",
    "changelog": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "clean": "(rm -r ./.nyc_output || true) && (rm -r ./coverage || true) && (rm -r dist || true)",
    "clean:all": "npm run clean && (rm -r ./node_modules || true)",
    "cover": "yarn test --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "postversion": "yarn cover -u && git commit . -m \"update snapshots after changing version\" && git push && git push --tags",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "upgrade": "npx npm-check -u",
    "preversion": "yarn test",
    "version": "yarn build",
    "patch": "npm version patch --no-git-tag-version",
    "minor": "npm version minor --no-git-tag-version",
    "major": "npm version major --no-git-tag-version"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.14.192",
    "@types/node": "18.19.33",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "axios": "^1.7.7",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.7.0",
    "msw": "^1.2.1",
    "prettier": "2.8.8",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
